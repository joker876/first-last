{
  "name": "first-last",
  "version": "1.1.0",
  "description": "Two array functions for selecting first or last n elements.",
  "main": "dist/index.min.js",
  "files": [
    "dist/*"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run build && npx jasmine",
    "start": "ts-node-dev --respawn src/index.ts",
    "build": "rmdir /s /Q dist & tsc && terser dist/index.js --output dist/index.min.js --config_file terserconfig.json --comments /^\\*\\n/ && cd dist && del /f index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joker876/first-last.git"
  },
  "bugs": {
    "url": "https://github.com/joker876/first-last/issues"
  },
  "homepage": "https://github.com/joker876/first-last#readme",
  "keywords": [
    "array",
    "last",
    "first"
  ],
  "author": "joker876",
  "license": "MIT",
  "devDependencies": {
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3",
    "@types/jasmine": "^4.0.3",
    "jasmine": "^4.1.0",
    "jasmine-ts-console-reporter": "^3.1.1"
  }
}
